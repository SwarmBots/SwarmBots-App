each bot in bots
	div(class="column", id= bot.name)
		table(class="table")
			tr
				th
					h2 #{bot.name}
			- if (bot.queue)
				each user in bot.queue
					tr
						td(class="queue", id= user.name)
							table
								tr
									td(rowspan=2) 
										img(src=#{user.photo})
									td #{user.name}
								tr
									td #{user.location}